        <script><![CDATA[function moveToAssess(){
	var check = g_form.getValue('u_choose_approver');
	if (check!=''){
		
		g_form.setValue("state", "-4");
		if (g_form.isNewRecord()) {
			g_form.save();
		} else {
			g_form.submit();
		}
	}else{
		alert('choose approver to send request approval');
		g_form.flash("u_choose_approver", "#FFFACD", 0);
	}
}
]]></script>
